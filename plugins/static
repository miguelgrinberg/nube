function nube_static {
    case "$1" in
        install)
            ;;

        detect)
            echo default
            ;;

        build)
            ;;

        deploy)
            APP=$2
            APP_ROOT=$3
            NUBE_NGINX_LOCATION=`cat << EOF
    location / {
        alias $APP_ROOT/;
        expires 30d;
    }
EOF
`
            ;;

        destroy)
            ;;
    esac
}
